#include <iostream>
using namespace std;

int main() {
    // 第一部分：星星三角形
    cout << "星星三角形：" << endl;
    for (int i = 1; i <= 6; ++i) {
        for (int j = 1; j <= i; ++j) {
            cout << "*";
        }
        cout << endl;
    }

    cout << endl; // 分隔空行

    // 第二部分：九九乘法表
    cout << "九九乘法表：" << endl;
    int i = 1, j;
    while (i <= 9) {
        j = 1;
        while (j <= 9) {
            cout << i << " * " << j << " = " << i * j << "\t";
            j++;
        }
        cout << endl;
        i++;
    }

    return 0;
}
